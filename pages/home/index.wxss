.home swiper {
  height: 340rpx;
}
.home swiper image {
  width: 100%;
}
.home .cate-wrapper {
  display: flex;
}
.home .cate-wrapper navigator {
  flex: 1;
  padding: 20rpx;
}
.home .cate-wrapper navigator image {
  width: 100%;
}
.home .floor-wrapper .floor .floor-title {
  padding: 10rpx 0;
}
.home .floor-wrapper .floor .floor-title image {
  width: 100%;
}
.home .floor-wrapper .floor .floor-list navigator {
  width: calc(100vw / 3);
  float: left;
}
.home .floor-wrapper .floor .floor-list navigator:nth-child(n+2) {
  height: calc(100vw / 3 * 386 / 232 /2);
  border-left: 10rpx solid #fff;
}
.home .floor-wrapper .floor .floor-list navigator:nth-child(2),
.home .floor-wrapper .floor .floor-list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.home .floor-wrapper .floor .floor-list navigator image {
  width: 100%;
  height: 100%;
}
