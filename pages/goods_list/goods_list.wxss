.goods-list .tabs .container .list-item {
  padding: 20rpx;
  border-bottom: 1rpx solid #f0f0f0;
}
.goods-list .tabs .container .list-item navigator {
  display: flex;
}
.goods-list .tabs .container .list-item navigator image {
  flex: 2;
  margin: 0 60rpx 0 30rpx;
}
.goods-list .tabs .container .list-item navigator .text {
  flex: 5;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.goods-list .tabs .container .list-item navigator .text .title {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.goods-list .tabs .container .list-item navigator .text .price {
  color: red;
}
.goods-list .tabs .container .not-more {
  height: 80rpx;
  font-size: 26rpx;
  text-align: center;
  line-height: 80rpx;
}
